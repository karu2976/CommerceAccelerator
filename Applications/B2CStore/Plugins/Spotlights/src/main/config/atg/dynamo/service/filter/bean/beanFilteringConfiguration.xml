<?xml version="1.0" encoding="UTF-8"?>

<bean-filtering>
  <repository name="/atg/content/ContentManagementRepository">
    <item-descriptor name="mediaContent" default-filter="summary">
      <filter id="summary">
        <property name="repositoryId"/>
        <property name="name"/>
        <property name="title"/>
        <property name="url"/>
        <property name="altText"/>
        <property name="imageSrcset"/>
        <property name="linkUrl"/>
        <property name="imageText1"/>
        <property name="imageText2"/>
        <property name="imageText3"/>
        <property name="imageText4"/>
      </filter>
    </item-descriptor>
  </repository>
</bean-filtering>
