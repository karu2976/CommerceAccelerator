<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE gsa-template SYSTEM "dynamosystemresource:/atg/dtds/gsa/gsa_1.0.dtd">
<gsa-template>
  <import-items>
    <add-item item-descriptor="siteConfiguration" id="basicSite">
      <set-property name="description"><![CDATA[CSA Basic Store]]></set-property>
      <set-property name="modificationTime"><![CDATA[01/01/2014 00:00:00]]></set-property>
      <set-property name="name"><![CDATA[CSA Basic Store]]></set-property>
      <set-property name="enabled"><![CDATA[true]]></set-property>

      <set-property name="defaultCatalog"><![CDATA[masterCatalog]]></set-property>
      <set-property name="defaultListPriceList"><![CDATA[listPrices]]></set-property>
      <set-property name="defaultSalePriceList"><![CDATA[salePrices]]></set-property>

      <set-property name="defaultCountry"><![CDATA[US]]></set-property>
      <set-property name="defaultLanguage"><![CDATA[en]]></set-property>
      <set-property name="languages"><![CDATA[en]]></set-property>

      <set-property name="contextRoot"><![CDATA[/csa]]></set-property>
      <set-property name="productionURL"><![CDATA[/csa/basic]]></set-property>
			<set-property name="additionalProductionURLs"><![CDATA[/csa/basic/en]]></set-property>
    </add-item>
  </import-items>
</gsa-template>
