<?xml version="1.0" encoding="UTF-8"?>

<bean-filtering>
    <repository name="/atg/commerce/catalog/ProductCatalog">
        <item-descriptor name="product" default-filter="summary">
            <filter id="productDetailsProduct">
                <property name="repositoryId"/>
                <property name="displayName"/>
                <property name="longDescription"/>
                <property name="largeImageUrl" target="largeImage.url"/>
            </filter>
        </item-descriptor>
    </repository>
</bean-filtering>
